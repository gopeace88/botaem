{
  "name": "botame-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "보탬e AI 가이드 어시스턴트 모노레포",
  "scripts": {
    "dev:admin": "pnpm --filter botame-admin dev",
    "dev:guide": "pnpm --filter botame-guide-app dev",
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @botame/core build",
    "build:admin": "pnpm --filter botame-admin build",
    "build:guide": "pnpm --filter botame-guide-app build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf node_modules dist out",
    "validate-playbooks": "node -r esbuild-runner/register scripts/validate-playbooks.ts"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}